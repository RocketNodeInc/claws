{
	"name": "@pterodactyl/claws",
	"description": "",
	"version": "0.0.1",
	"license": "MIT",
	"main": "dist/index.mjs",
	"private": true,
	"scripts": {
		"postinstall": "husky install",
		"clean": "rm -f dist/*",
		"build": "cross-env NODE_ENV=\"development\" USER_AGENT=\"undefined\" $npm_node_execpath build.mjs",
		"build:production": "$npm_execpath run clean && cross-env NODE_ENV=\"production\" USER_AGENT=\"Pterodactyl Claws (https://pterodactyl.io)\" $npm_node_execpath build.mjs",
		"dev": "miniflare --debug --watch --build-command \"$npm_execpath run build\"",
		"lint": "eslint ./{src,types}/**/*.ts --ext .ts --fix",
		"format": "prettier --write --ignore-unknown ./build.mjs ./{src,types}/**/*.ts"
	},
	"dependencies": {
		"itty-router": "^2.3.10",
		"itty-router-extras": "^0.4.2",
		"~": "link:./src"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^2.2.2",
		"@typescript-eslint/eslint-plugin": "^4.28.5",
		"@typescript-eslint/parser": "^4.28.5",
		"cross-env": "^7.0.3",
		"esbuild": "^0.12.15",
		"eslint": "^7.31.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-config-standard": "^16.0.3",
		"eslint-plugin-import": "^2.23.4",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-promise": "^5.1.0",
		"eslint-plugin-simple-import-sort": "^7.0.0",
		"husky": "^7.0.1",
		"miniflare": "^1.3.2",
		"prettier": "^2.3.2",
		"typescript": "^4.3.5"
	},
	"engines": {
		"node": ">=12",
		"yarn": "^1.22.x"
	},
	"lint-staged": {
		"./{src,types}/**/*.ts": [
			"npm run lint",
			"npm run format"
		]
	}
}
