{
	"name": "@pterodactyl/claws",
	"description": "",
	"version": "0.0.1",
	"license": "MIT",
	"main": "dist/index.mjs",
	"type": "module",
	"private": true,
	"scripts": {
		"postinstall": "husky install",
		"clean": "rm -f dist/*",
		"build": "cross-env ESBUILD_FORMAT=\"esm\" NODE_ENV=\"development\" USER_AGENT=\"undefined\" $npm_node_execpath build.cjs",
		"build:production": "$npm_execpath run clean && cross-env ESBUILD_FORMAT=\"esm\" NODE_ENV=\"production\" USER_AGENT=\"Pterodactyl Claws (https://pterodactyl.io)\" $npm_node_execpath build.cjs",
		"dev": "miniflare --debug --watch --build-command \"$npm_execpath run build\""
	},
	"dependencies": {
		"itty-router": "2.4.2",
		"itty-router-extras": "0.4.2",
		"~": "link:./src"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "2.2.2",
		"@types/itty-router-extras": "^0.4.0",
		"@typescript-eslint/eslint-plugin": "4.31.2",
		"@typescript-eslint/parser": "4.31.2",
		"@yarnpkg/esbuild-plugin-pnp": "1.0.1-rc.3",
		"cross-env": "7.0.3",
		"esbuild": "0.13.2",
		"eslint": "7.32.0",
		"eslint-config-prettier": "8.3.0",
		"eslint-config-standard": "16.0.3",
		"eslint-plugin-import": "2.24.2",
		"eslint-plugin-node": "11.1.0",
		"eslint-plugin-promise": "5.1.0",
		"eslint-plugin-simple-import-sort": "7.0.0",
		"husky": "7.0.2",
		"miniflare": "1.4.1",
		"prettier": "2.4.1",
		"typescript": "4.4.3"
	},
	"engines": {
		"node": ">=14"
	},
	"lint-staged": {
		"**/*.{mjs,ts}": [
			"yarn eslint --fix",
			"yarn prettier --write"
		],
		"*.json": [
			"yarn prettier --write"
		],
		"*.md": [
			"yarn prettier --write"
		]
	},
	"packageManager": "yarn@3.1.0-rc.1"
}
